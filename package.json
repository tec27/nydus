{
  "name": "nydus",
  "version": "4.0.2",
  "description": "WebSocket server using the nydus protocol, a simple RPC/PubSub protocol.",
  "main": "dist/cjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 12.4.0"
  },
  "scripts": {
    "autotest": "ts-mocha -p tsconfig-cjs.json -w --watch-extensions ts -R spec test/**.ts",
    "build": "tsc && tsc -p tsconfig-cjs.json",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.ts ./",
    "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run clean && pnpm run build",
    "test": "ts-mocha -p tsconfig-cjs.json -R spec test/**.ts",
    "testonly": "ts-mocha -p tsconfig-cjs.json -R spec test/**.ts",
    "posttest": "pnpm run tsc --noEmit && pnpm run lint",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus"
  },
  "keywords": [
    "nydus",
    "websockets",
    "websocket",
    "rpc",
    "pubsub"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus/issues"
  },
  "homepage": "https://github.com/tec27/nydus",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "backo2": "^1.0.2",
    "cuid": "^2.1.6",
    "engine.io": "^6.2.0",
    "immutable": "^4.0.0-rc.12",
    "nydus-protocol": "^2.0.0",
    "ruta3": "^2.0.0",
    "typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@types/backo2": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/engine.io": "^3.1.4",
    "@types/engine.io-client": "^3.1.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "engine.io-client": "^6.2.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "nydus-client": "^4.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
