{
  "name": "nydus",
  "version": "1.1.0",
  "description": "WebSocket server using the nydus protocol, a simple RPC/PubSub protocol.",
  "main": "dist/cjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 12.4.0"
  },
  "scripts": {
    "autotest": "ts-mocha --p tsconfig-cjs.json w --watch-extensions ts -R spec test/**.ts",
    "build": "tsc && tsc -p tsconfig-cjs.json",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.ts ./",
    "prepublishOnly": "yarn run lint && yarn run test && yarn run clean && yarn run build",
    "test": "ts-mocha -p tsconfig-cjs.json -R spec test/**.ts",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus"
  },
  "keywords": [
    "nydus",
    "websockets",
    "websocket",
    "rpc",
    "pubsub"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus/issues"
  },
  "homepage": "https://github.com/tec27/nydus",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "backo2": "^1.0.2",
    "cuid": "^2.1.6",
    "engine.io": "2Pacalypse-/engine.io#master",
    "immutable": "^4.0.0-rc.12",
    "nydus-protocol": "^2.0.0",
    "ruta3": "^2.0.0"
  },
  "devDependencies": {
    "@types/backo2": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/engine.io-client": "^3.1.4",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "engine.io-client": "^4.0.5",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "nydus-client": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.3"
  }
}
